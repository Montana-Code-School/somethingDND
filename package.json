{
  "name": "heroku-cra-node",
  "version": "1.0.0",
  "description": "How to use create-react-app with a custom Node API on Heroku",
  "engines": {
    "node": "8.9.x"
  },
  "scripts": {
    "start": "node server.js",
    "heroku-postbuild": "cd generator/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "devstart": "./node_modules/nodemon/bin/nodemon.js server"
  },
  "cacheDirectories": [
    "node_modules",
    "generator/node_modules"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "express": "^4.14.1",
    "lodash": "^4.17.5",
    "mongodb": "^3.0.3",
    "mongoose": "^5.0.7",
    "nodemon": "^1.17.1",
    "request": "^2.83.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mars/heroku-cra-node.git"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {},
  "bugs": {
    "url": "https://github.com/mars/heroku-cra-node/issues"
  },
  "homepage": "https://github.com/mars/heroku-cra-node#readme",
  "main": "index.js",
  "author": ""
}
